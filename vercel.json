{
  "env": {
    "NEXTAUTH_URL": "https://navada-robotics.vercel.app"
  },
  "build": {
    "env": {
      "NEXTAUTH_URL": "https://navada-robotics.vercel.app",
      "SKIP_ENV_VALIDATION": "1",
      "NEXT_TELEMETRY_DISABLED": "1",
      "NODE_ENV": "production",
      "SKIP_VALIDATION": "true"
    }
  },
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm install --legacy-peer-deps",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"]
}