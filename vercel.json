{
  "env": {
    "NEXTAUTH_URL": "https://navada-robotics.vercel.app"
  },
  "build": {
    "env": {
      "NEXTAUTH_URL": "https://navada-robotics.vercel.app"
    }
  },
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "headers": [
    {
      "source": "/videos/(.*\\.(mp4|MP4|avi|mov|wmv|flv|webm|mkv))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Content-Type",
          "value": "video/mp4"
        },
        {
          "key": "Accept-Ranges",
          "value": "bytes"
        }
      ]
    }
  ]
}